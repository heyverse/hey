{
  "name": "@hey/api",
  "version": "0.0.0",
  "private": true,
  "license": "AGPL-3.0",
  "scripts": {
    "build": "echo 'Not required'",
    "codegen": "prisma generate --schema ./src/prisma/schema.prisma",
    "dev": "tsx watch src/index.ts",
    "postinstall": "pnpm codegen",
    "prisma:clean": "prisma migrate reset --schema ./src/prisma/schema.prisma",
    "prisma:format": "prisma format --schema ./src/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema ./src/prisma/schema.prisma",
    "start": "tsx watch src/index.ts",
    "typecheck": "tsc --pretty"
  },
  "dependencies": {
    "@aws-sdk/client-sts": "^3.808.0",
    "@hey/data": "workspace:*",
    "@hey/helpers": "workspace:*",
    "@hey/indexer": "workspace:*",
    "@hono/node-server": "^1.14.1",
    "@hono/zod-validator": "^0.5.0",
    "@lens-protocol/metadata": "^2.0.0",
    "@prisma/client": "^6.7.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "hono": "^4.7.9",
    "hono-rate-limiter": "^0.4.2",
    "jose": "^6.0.11",
    "linkedom": "^0.18.10",
    "pg-promise": "^11.13.0",
    "redis": "^5.0.1",
    "tsx": "^4.19.4",
    "viem": "^2.29.2",
    "xmlbuilder2": "^3.1.1",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@hey/config": "workspace:*",
    "@hey/types": "workspace:*",
    "@types/cors": "^2.8.18",
    "@types/node": "^22.15.18",
    "prisma": "^6.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
