{
  "name": "@hey/api",
  "version": "0.0.0",
  "private": true,
  "license": "AGPL-3.0",
  "scripts": {
    "build": "echo 'Not required'",
    "codegen": "prisma generate --schema ./src/prisma/schema.prisma",
    "dev": "tsx watch src/index.ts",
    "postinstall": "pnpm codegen",
    "prisma:clean": "prisma migrate reset --schema ./src/prisma/schema.prisma",
    "prisma:format": "prisma format --schema ./src/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema ./src/prisma/schema.prisma",
    "start": "tsx watch src/index.ts",
    "typecheck": "tsc --pretty"
  },
  "dependencies": {
    "@aws-sdk/client-sts": "^3.787.0",
    "@hey/data": "workspace:*",
    "@hey/helpers": "workspace:*",
    "@lens-protocol/metadata": "^2.0.0",
    "@prisma/client": "^6.6.0",
    "@trpc-limiter/memory": "^1.0.0",
    "@trpc/server": "^11.1.0",
    "axios": "^1.8.4",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "graphql": "^16.10.0",
    "jose": "^6.0.10",
    "linkedom": "^0.18.9",
    "tsx": "^4.19.3",
    "viem": "^2.26.3",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@hey/config": "workspace:*",
    "@hey/types": "workspace:*",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/node": "^22.14.1",
    "prisma": "^6.6.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
